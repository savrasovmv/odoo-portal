<?xml version="1.0" ?>
<odoo>
    <data>

        <template id="social_assets_frontend" inherit_id="website.assets_frontend" name="Social Frontend Assets">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/scss" href="/website_social/static/src/scss/social.scss" t-ignore="true"/>
            </xpath>
            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/website_social/static/src/js/social_post_form.js"></script>
                <script type="text/javascript" src="/website_social/static/src/js/social_post.js"></script>
            </xpath>
        </template>

    </data>
</odoo>
