<?xml version="1.0" ?>
<odoo>
    <data>

        <template id="assets_frontend" inherit_id="website.assets_frontend" name="Adbook Frontend Assets">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/scss" href="/website_adbook/static/src/scss/adbook.scss" t-ignore="true"/>
            </xpath>
            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/website_adbook/static/src/js/menu.js"></script>
                <script type="text/javascript" src="/website_adbook/static/src/js/search.js"></script>
            </xpath>
        </template>

    </data>
</odoo>
