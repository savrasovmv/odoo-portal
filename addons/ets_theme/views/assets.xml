<?xml version="1.0" ?>
<odoo>
    <data>

        <template id="assets_frontend" name="ETS style" inherit_id="website.assets_frontend">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/scss" href="/ets_theme/static/src/scss/ets.scss"/>
            </xpath>
        </template>

        <template id="assets_backend" name="ETS style" inherit_id="web.assets_backend">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/scss" href="/ets_theme/static/src/scss/ets_backend.scss"/>
            </xpath>
        </template>

    </data>
</odoo>

